"wpackagist-plugin/widget-importer-exporter":"1.5.5"
  },
  "extra": {
    "installer-paths": {
      "web/app/mu-plugins/{$name}/": ["type:wordpress-muplugin"],
      "web/app/plugins/{$name}/": ["type:wordpress-plugin"],
      "web/app/themes/{$name}/": ["type:wordpress-theme"]
    },
 "scripts": {
    "pre-update-cmd": [
      "mkdir -p /tmp/wordpress-packages"
    ],
    "pre-install-cmd": [
      "mkdir -p /tmp/wordpress-packages",
      "aws s3 sync s3://mla-backup/commons/packages /tmp/wordpress-packages --only-show-errors "
    ]
    },
    "scripts-dev": {
      "post-install-cmd": [
        "chmod a+w web/wp/wp-includes/template.php"
      ]
    },
    "wordpress-install-dir": "web/wp"
  }
}






